{
  "expo": {
    "name": "AwesomeProject",
    "slug": "AwesomeProject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/img/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/img/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "softwareKeyboardLayoutMode": "pan"
      },
      "config": {
        "googleMaps": { "apiKey": "xxx" }
      }
    },
    "web": {
      "favicon": "./src/img/splash.png"
    }
  }
}
